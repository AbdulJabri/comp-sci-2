package acsl.chessQueen;

public class Chess {

	/**
	 * @author Drew Higgins
	 * ACSL Chess Queen
	 * 10/31/2013
     *           _
     *          /))
     *       __(((__
     *     .' _`""`_`'.
     *    /  /\\  /\\  \
     *   |  /)_\\/)_\\  |
     *   |  _  _()_  _  |
     *   |  \\/\\/\\//  |
     *    \  \/\/\/\/  /
     *     '.___..___.'
	 */
	public static void main(String[] args) {
		boolean[][] chessBoard = new boolean[8][8];
		for(int i = 0; i < 8; i++){
			for(int j = 0; j < 8; j++){
				System.out.print(chessBoard[i][j] + " ");
			}
			System.out.println();
		}
	}

}
